.custom-navbar {
    --td-navbar-color: #fff;
    --td-navbar-bg-color: #a5040d;
}

.minetitle {
    height: 300rpx;
    background-color: #a5040d;
    color: white;
    width: 100%;
    position: fixed;
    top: 90px;
}

.body {
    background-color: transparent;
    margin-top: 180px;
}

.foot {
    position: fixed;
    bottom: 0px;
    padding-bottom: 20px;
    background-color: white;
    padding-top: 10px;
}

.button-example {
    margin: 0 32rpx;
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 32rpx;
}

.button-example:not(:last-child) {
    margin-bottom: 32rpx;
}